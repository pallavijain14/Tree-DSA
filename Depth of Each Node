    /*
    Definition for TreeNode.
    static public class TreeNode {
        public int val;
        public TreeNode left;
        public TreeNode right;
        TreeNode() {}
        TreeNode(int val) { this.val = val; this.depth = -1;}
    }
    */

   static void dfs(TreeNode A, int d){
        A.depth = d;
        if(A.left != null){
            dfs(A.left, d + 1);    
        }
        if(A.right != null){
            dfs(A.right, d + 1);
        }
    }
    
    static void solve(TreeNode A){
        dfs(A, 1);
    }


        
